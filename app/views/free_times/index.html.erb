  <title>jQuery demo</title>
</head>
<body>
  <script src="http://code.jquery.com/jquery-1.7.2.min.js"></script>
<script>
    $(document).ready(function(){
      $("h2.week1_click").click(function(event){
        $("h5.week1_hide").toggle();
        $("h2.week1_hide").toggle();
        event.preventDefault();
      });
    });
    $(document).ready(function(){
      $("h2.week2_click").click(function(event){
        $("h5.week2_hide").toggle();
        $("h2.week2_hide").toggle();
        event.preventDefault();
